{
  "name": "al_lambda_template",
  "homepage": "https://github.com/alertlogic/al_lambda_template",
  "author": "Barry Skidmore <bskidmore@alertlogic.com>",
  "description": "Base framework for al_lambda_template Development.",
  "bugs": {
    "url": "https://github.com/alertlogic/al_lambda_template"
  },
  "scripts": {
    "build": "npm run test && node build/build.js && node build/zip.js",
    "hookmeup": "cd utility; ./hookmeup.sh",
    "karma": "karma start --single-run",
    "lint": "node node_modules/.bin/jshint --config ./.jshintrc --exclude-path ./.jshintignore src/main/",
    "postinstall": "parallelshell 'npm run hookmeup'",
    "release": "npm version patch",
    "release:build": "npm version patch && npm run build",
    "test": "npm run lint && npm run karma"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alertlogic/al_lambda_template.git"
  },
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "5.9.0"
  },
  "dependencies": {
    "graceful-fs": "^4.1.3",
    "lodash": "^4.6.1",
    "bower": "^1.7.7",
    "glob": "3.1.21",
    "glob-all": "3.0.1",
    "jasmine-core": "^2.4.1",
    "jasmine-reporters": "^2.1.1",
    "jshint": "^2.9.1",
    "karma": "^0.13.22",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.0",
    "karma-requirejs": "^0.2.5",
    "lazystream": "^1.0.0",
    "mkdirp": "^0.5.1",
    "onchange": "^2.1.2",
    "parallelshell": "^2.0.0",
    "process": "^0.11.2",
    "readable-stream": "^2.0.1",
    "replacestream": "^4.0.0",
    "requirejs": "^2.1.22",
    "rimraf": "^2.5.2",
    "uglify-js": "^2.6.2"
  },
  "folders": {
    "build": "target/",
    "translations": "po/",
    "wwwRoot": "src/main/",
    "jsSource": "src/main/",
    "testRoot": "src/test/"
  },
  "devDependencies": {}
}
